{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:earlyMondays"
  ],
"packageRules": [
    {
      "matchDatasources": ["maven"],
      "registryUrls": [
        "https://maven.pkg.github.com/abhishekmalvadkar/ihmsservicecommon"
      ]
    }
  ],
    "hostRules": [
  {
    "hostType": "maven",
    "matchHost": "https://maven.pkg.github.com/abhishekmalvadkar/ihmsservicecommon*",
    "token": "${{secrets.RENOVATE_GITHUB_TOKEN}}",
  },
]
}
